{
  "command": {
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "createRelease": "github",
      "message": "chore(release): bump version [skip ci]",
      "ignoreChanges": ["**/package.json", "**/package-lock.json"]
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
